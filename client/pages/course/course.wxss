.iconfont {
  font-family:"iconfont" !important;
  font-size:38rpx;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-star:before { content: "\e6b0"; }

.icon-lock:before { content: "\e657"; }

page{
    /*background: radial-gradient( yellow, yellow, goldenrod);*/
    background: radial-gradient( rgba(255,255,255,0.5), rgba(255,255,0,0.2), rgba(218,165,32,0.5));
    height:100%;
}

.course-wrap{
    height:100%;
    overflow:hidden;
}
.container{
    display:flex;
    flex-direction: row;
    align-content: space-around;
    justify-content: center;
    flex-wrap: wrap;
    /*background-image: radial-gradient( yellow, yellow, goldenrod);*/
    padding:30rpx 20rpx;
}
@keyframes tap {
  0% {
    transform: rotate(10deg) scale(1);
  }

  50% {
    transform: rotate(10deg) scale(1.1);
  }

  100% {
    transform: rotate(10deg) scale(1);
  }
}

.btn-taped {
  animation: tap 100ms ease-in;
}

.list{
    display:flex;
    justify-content: center;
    width:150rpx;
    align-content: space-around;
}
.list-wrap{
    display:flex;
    margin:20rpx 0;
    width:120rpx;
    height:120rpx;
    flex-direction: column;
    align-content: space-around;
    justify-content: center;
    border:6rpx solid rgb(90, 20, 11);
    border-radius: 25rpx;
    background:#fff;
    transform: rotate(10deg);
    box-shadow: -5rpx 5rpx 3rpx goldenrod;
    position: relative;
    overflow: hidden;
    opacity: 0.6;
}
.list-wrap.solved, .list-wrap.unlock{
    opacity: 1;
}
.score{
    display:flex;
    align-content:center;
    text-align:center;
    height:70rpx;
    align-items: center;
    justify-content: center;
    background:lightgray;
    border-radius: 19rpx 19rpx 0 0;
}
.num{
    color:#333;
    /*text-shadow: 5rpx 5rpx 0 #333, -5rpx -5rpx 0 #333;*/
    font-family: "Helvetica","Helvetica Neue","PingFang SC","微软雅黑";
    font-weight: 800;
    -webkit-font-smoothing: antialiased;
    /*
    -webkit-text-stroke: 2rpx #333;
    */
    font-size:40rpx;
}
.solved .score{
    opacity:1;
    background:lightgreen;
}
.solved .num{
    color:darkgreen;
    /*
    -webkit-text-stroke: 2rpx darkgreen;
    */
}
.unlock .score{
    /*
    background:lightblue;
    */
}
.star {
    display: flex;
    flex-direction: row;
    align-content: space-around;
    justify-content: center;
    border-top:2rpx solid gray;
    background:lightyellow;
    height:50rpx;
    align-items: center;
    border-radius: 0 0 25rpx 25rpx;
}
.solved .star{
    border-top:2rpx solid goldenrod;
}
.star .icon-star:before{
    color:#f1f1f1;
    -webkit-text-stroke: 4rpx lightgray;
}
.star.light .icon-star:before{
    color: gold;
    -webkit-text-stroke: 4rpx rgb(218, 156, 0);
}
.icon-lock:before{
    color:#333;
}