}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 38rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-star:before {
  content: "\e6b0";
}
.icon-huangguan:before { content: "\e61a"; }

page {
  background: radial-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 0, 0.2), rgba(218, 165, 32, 0.5));
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: column;
  justify-content: flex-start;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.container.layout-center {
  justify-content: center;
}

@keyframes tap {
  0% {
    transform: rotate(2deg) scale(1);
  }

  50% {
    transform: rotate(2deg) scale(1.1);
  }

  100% {
    transform: rotate(2deg) scale(1);
  }
}

.btn-taped {
  animation: tap 200ms ease-in;
}

.userinfo, .uploader, .tunnel {
  margin-top: 40rpx;
  height: 140rpx;
  width: 100%;
  border-left: none;
  border-right: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
  line-height: 140rpx;
}

.userinfo-avatar {
  width: 100rpx;
  height: 100rpx;
  margin: 20rpx;
  border-radius: 50%;
  border: 5rpx solid rgba(255, 255, 255, 0.8);
}

.userinfo-nickname {
  font-size: 32rpx;
  color: #333;
}

.userinfo-nickname::after {
  border: none;
}

.guide-wrap {
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  margin-top: -400rpx;
}

.guide-show{
  visibility: visible;
}
.guide-hide{
  visibility: hidden;
}

.guide-text {
  border: 20rpx solid rgba(255, 255, 255, 0.9);
  border-radius: 20rpx;
  background: rgba(255, 255, 255, 0.65);
  width: 580rpx;
  height: 400rpx;
  padding: 20rpx;
  transform: rotate(-2deg);
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  line-height: 1.5;
}

.text-title {
  text-align: center;
  font-size: 40rpx;
  padding: 20rpx 0;
}

.text-content {
  color: rgb(90, 20, 11);
}

.user-enter {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 200rpx;
  height: 200rpx;
  line-height: 200rpx;
  border: 10rpx solid rgb(90, 20, 11);
  border-radius: 50%;
  /*
    background: radial-gradient(rgb(224, 81, 0) 80%, rgba(255, 255, 255, 0.8) 20%);
    */
  background: rgba(255, 81, 0, 0.9);
  margin: -50rpx 0 0 0;
  padding: 0rpx;
  color: rgb(255, 255, 255);
  font-weight: 700;
  font-size: 40rpx;
  box-shadow: -5rpx 5rpx 5rpx rgba(0, 0, 0, 1);
  text-shadow: -1rpx 1rpx 1rpx rgba(0, 0, 0, 0.8);
  transform: rotate(-5deg);
}
.user-enter.button-hover{
  transform: scale(1.1) rotate(-5deg);
}

.user-enter::after {
  border: none;
}

.user-menu {
  /*
    margin-top:50rpx;
    */
}

.userinfo-wrap {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.userinfo-score {
  padding-right: 20rpx;
  display: flex;
  flex-direction: row;
  font-size: 32rpx;
  width: 300rpx;
  color: #333;
  margin-top: 45rpx;
  align-items: center;
  justify-content: flex-end;
  flex-wrap: nowrap;
  height: 140rpx;
  line-height: 140rpx;
}

.score-num {
  padding-left: 10rpx;
}


/**
 * 游戏菜单导航
 */
.navigators {
  padding-top: 60rpx;
  padding-bottom: 100rpx;
}

.navigator-text {
  margin-top: 25rpx;
  padding: 0 20rpx;
  width: 600rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 100rpx;
  align-items: center;
  border: 8rpx solid rgb(90, 20, 11);
  border-radius: 30rpx;
  background: rgba(255, 81, 0, 0.8);
  transform: rotate(2deg);
  box-shadow: -5rpx 5rpx 3rpx goldenrod;
  font-weight: 700;
  opacity: 0.3;
  transition: opacity 500ms ease-out;
}
.navigator-text .title, .navigator-text .score{
  color: rgba(255, 255, 255, 0.8); /*rgb(218, 156, 0);*/
}

.enable {
  opacity: 1;
}

.solved {
  /*
    background: linear-gradient(rgba(106, 247, 106, 0.8), rgba(106, 247, 106, 0.3), rgba(106, 247, 106, 0.8));
    */
  background: lightgreen; /*rgba(57, 211, 57, 0.8);*/
  position: relative;
  text-shadow: inset 0 0 5rpx gold;
}
.solved .title, .solved .score{
  color: darkgreen;
}

.star {
  color: rgb(255, 204, 75);
  /*
    -webkit-text-stroke: 4rpx rgb(218, 156, 0);
    */
  -webkit-text-stroke: 4rpx rgba(255, 255, 255, 0.8);
  margin-right: 5rpx;
}

.solved .star {
  color: gold;
  -webkit-text-stroke: 4rpx rgb(218, 156, 0);
}
.totally-solved::after{
  position: absolute;
  content: "\e7aa";
  font-size:60rpx;
  top: -34rpx;
  right: -34rpx;
  font-weight: 700;
  transform: rotate(45deg);
  color: gold;
  -webkit-text-stroke: 2rpx goldenrod;
  text-shadow: 0 3rpx 3rpx rgb(90, 20, 11);
}
