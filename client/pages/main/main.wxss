.iconfont {
  font-family: "iconfont" !important;
  font-size: 0rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconfont:before {
  font-size: 16px;
}

.icon-multiply:before {
  content: "\e619";
}

.icon-plus:before {
  content: "\e64d";
}

.icon-divide:before {
  content: "\e63d";
}

.icon-minus:before {
  content: "\e600";
}

.icon-reset:before {
  content: "\e612";
}

.icon-redo:before {
  content: "\e615";
}

.icon-next:before {
  content: "\e626";
}

.icon-undo:before {
  content: "\e64c";
}

.icon-pre:before {
  content: "\e710";
}

.icon-star:before {
  content: "\e6b0";
}

.icon-lock:before {
  content: "\e657";
}

.icon-help:before {
  content: "\e605";
}

.icon-fenxiang:before {
  content: "\e621";
}

page {
  background: radial-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 0, 0.2), rgba(218, 165, 32, 0.5));
  height: 100%;
}

.container {
  display: flex;
  flex-direction: column;
  align-content: space-around;
  justify-content: center;
  height: 100%;
  overflow: hidden;
}

.tool-wrap, .operator-wrap, .numbers-wrap {
  display: flex;
  flex-direction: row;
  padding: 20rpx;
}

@keyframes tap {
  0% {
    transform: scale(1);
  }

  50% {
    transform: scale(1.1);
  }

  100% {
    transform: scale(1);
  }
}

@keyframes shake {
  0% {
    margin: 0;
  }

  20% {
    margin-left: -10rpx;
  }

  40% {
    margin-right: -10rpx;
  }

  60% {
    margin-left: -10rpx;
  }

  80% {
    margin-right: -10rpx;
  }

  100% {
    margin: 0;
  }
}

.btn-taped {
  animation: tap 200ms ease-in;
}

/**
 * tool
 */

.tool-wrap {
  position: absolute;
  top: 20rpx;
  /*background:#ffe484;*/
  align-content: center;
  justify-content: center;
}

.tool-wrap view {
  width: 120rpx;
  height: 120rpx;
  margin-left: 5rpx;
  text-align: center;
  line-height: 120rpx;
}

.tool-wrap .iconfont:before {
  font-size: 80rpx;
  color: #2e0000;
  /*color:#e89000;*/
}

.tool-wrap view.disabled .iconfont:before {
  opacity: 0.5;
}

.tool-undo .iconfont:before, .tool-redo .iconfont:before {
  font-size: 90rpx;
}

.tool-reset .iconfont:before {
  font-weight: 700;
}

.tool-tap .iconfont:before {
  opacity: 0.8;
}

/**
 * score
 */

.score-wrap .no {
  padding-right: 20rpx;
  color: #2e0000;
}

.no-wrap {
  text-align: right;
}

.time-wrap {
  text-align: left;
}

.no-wrap.disappear, .time-wrap.disappear {
  visibility: hidden;
}

.score-star .icon-star {
  position: absolute;
  z-index: 1000;
  top: -100rpx;
}

.score-star .star-1 {
  transition: top 200ms ease-in;
}

.score-star .star-2 {
  transition: top 200ms ease-in 100ms;
}

.score-star .star-3 {
  transition: top 200ms ease-in 200ms;
}

.score-star.show .icon-star {
  top: 200rpx;
}

.score-1 .star-1 {
  left: 328rpx;
}

.score-1 .star-2 {
  visibility: hidden;
}

.score-1 .star-3 {
  visibility: hidden;
}

.score-2 .star-1 {
  left: 278rpx;
}

.score-2 .star-2 {
  left: 378rpx;
}

.score-2 .star-3 {
  visibility: hidden;
}

.score-3 .star-1 {
  left: 228rpx;
}

.score-3 .star-2 {
  left: 328rpx;
}

.score-3.show .star-2 {
  top: 180rpx;
}

.score-3 .star-3 {
  left: 428rpx;
}

.score-star .icon-star::before {
  font-size: 100rpx;
  color: gold;
  text-shadow: -3rpx 3rpx 3rpx #e88f00;
}

/**
 * game tip
 */

@keyframes shine {
  0% {
    text-shadow: 0 0 5rpx goldenrod;
  }

  20% {
    transform: rotate(2deg);
    text-shadow: 0 0 50rpx goldenrod;
  }

  80% {
    transform: rotate(-2deg);
    text-shadow: 0 0 50rpx goldenrod;
  }

  100% {
    text-shadow: 0 0 5rpx goldenrod;
  }
}

.goto-home{
  position: absolute;
  left: 60rpx;
  top: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: flex-end;
}

.game-tip {
  position: absolute;
  right: 60rpx;
  margin-top: -98rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: flex-end;
}

.get-btn {
  font-size: 32rpx;
  border: none;
  background: transparent;
  display: inline-block;
  height: 40rpx;
  box-sizing: content-box;
  padding: 0;
  margin: 0;
  line-height: 40rpx;
  color: #390707;
  animation: shine 1s ease-in-out infinite;
  /*border:2rpx solid #e88f00;

font-size:32rpx;
background:rgba(255, 181, 50, 0.8);
padding:5rpx 10rpx;

line-height:32rpx;
color:rgba(182, 5, 5, 0.6);
box-shadow: 3rpx 3rpx 3rpx rgba(117, 38, 1, 0.8);
*/
  opacity: 1;
}

.get-btn::after {
  border: none;
}

.get-btn.icon-fenxiang:before {
  font-size: 32rpx;
}

.get-btn.button-hover {
  opacity: 0.5;
  animation: tap 200ms ease-in;
}

.get-home::before{
  content: "\e64c";
}

.get-expression {
  margin-left: 10rpx;
  margin-top: 10rpx;
}

/**
 * number display
 */

.numbers-wrap {
  width: 640rpx;
  height: 640rpx;
  margin-bottom: 60rpx;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.numbers-top, .numbers-bottom {
  width: 100%;
  height: 312rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  align-content: space-between;
  justify-content: center;
}

.numbers-top {
  margin-bottom: 20rpx;
}

.numbers-wrap .number {
  width: 300rpx;
  height: 300rpx;
  position: absolute;
  border: 6rpx solid #2e0000;
  background: #ffe585;
  border-radius: 20rpx;
  text-align: center;
  line-height: 300rpx;
  box-shadow: 5rpx 5rpx 5rpx rgba(46, 0, 0, 0.5);
  opacity: 0.95;
}

.numbers-wrap .calculate {
  /*
    background:rgba(255, 81, 0, 0.8);
    background:lightgreen;
    */
  background: #fcc100;
}

.numbers-wrap .calculate.wrong{
  animation: shake 100ms ease-in-out 3;
}

.numbers-wrap .disappear {
  visibility: hidden;
}

.numbers-wrap .icon-num {
  font-size: 200rpx;
  color: #390707;
  font-family: "Helvetica", "Helvetica Neue", "PingFang SC", "微软雅黑";
  font-weight: 800;
  -webkit-font-smoothing: antialiased;
}

.numbers-wrap .size-3 .icon-num {
  font-size: 150rpx;
}

.numbers-wrap .size-4 .icon-num {
  font-size: 110rpx;
}

.numbers-wrap .size-5 .icon-num {
  font-size: 90rpx;
}

.numbers-wrap .size-6 .icon-num {
  font-size: 60rpx;
}

.numbers-wrap .size-7 .icon-num {
  font-size: 58rpx;
}

.numbers-wrap .size-8 .icon-num {
  font-size: 54rpx;
}

.numbers-wrap .size-9 .icon-num {
  font-size: 52rpx;
}

.numbers-wrap .size-10 .icon-num {
  font-size: 48rpx;
}

.numbers-wrap .size-11 .icon-num {
  font-size: 45rpx;
}

/*
.numbers-wrap .calculate .icon-num {
    color:rgba(255,255,255,0.8);
}
*/

.numbers-wrap .calculate .icon-num {
  color: #2e0000;
}

.numbers-wrap .number-tap {
  opacity: 1;
}

.numbers-wrap .number-tl {
  left: 20rpx;
  top: 20rpx;
  transition: left 0.5s, top 0.5s;
}

.numbers-wrap .number-tr {
  right: 20rpx;
  top: 20rpx;
  transition: right 0.5s, top 0.5s;
}

.numbers-wrap .number-bl {
  left: 20rpx;
  bottom: 20rpx;
  transition: left 0.5s, bottom 0.5s;
}

.numbers-wrap .number-br {
  right: 20rpx;
  bottom: 20rpx;
  transition: right 0.5s, bottom 0.5s;
}

.numbers-wrap .number-tl.solved {
  left: 184rpx;
  top: 184rpx;
}

.numbers-wrap .number-tr.solved {
  right: 184rpx;
  top: 184rpx;
}

.numbers-wrap .number-bl.solved {
  left: 184rpx;
  bottom: 184rpx;
}

.numbers-wrap .number-br.solved {
  right: 184rpx;
  bottom: 184rpx;
}

/**
 * operator area
 */

.operator-wrap {
  width: 660rpx;
  height: 140rpx;
  position: absolute;
  bottom: 40rpx;
}

.operator-wrap .iconfont:before {
  font-size: 100rpx;
  color: #e88f00;
}

.operator-wrap view.button {
  width: 25%;
  height: 140rpx;
  text-align: center;
}

.operator-wrap .block {
  width: 130rpx;
  height: 130rpx;
  text-align: center;
  line-height: 130rpx;
  align-items: center;
  align-content: center;
  justify-content: center;
}

.plus .iconfont:before {
  font-size: 90rpx;
}

.minus .iconfont:before, .divide .iconfont:before {
  font-weight: 800;
  -webkit-text-stroke: 4rpx #e88f00;
}

.operator-wrap .selected .block {
  height: 130rpx;
  width: 130rpx;
  /*
    background:#ffe37e;
    */
  background: rgba(255, 81, 0, 0.8);
  /*border:5rpx solid rgba(255, 81, 0, 0.8);/* #ffe37e;*/
  border-radius: 10rpx;
}

.operator-wrap .selected .block .iconfont:before {
  color: rgba(255, 255, 255, 0.8);
}

.minus.selected .iconfont:before, .divide.selected .iconfont:before {
  color: rgba(255, 255, 255, 0.8);
  -webkit-text-stroke: 4rpx rgba(255, 255, 255, 0.8);
}

